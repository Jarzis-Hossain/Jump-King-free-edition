<?xml version="1.0"?>
<ScrollingBGdata xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:xsd="http://www.w3.org/2001/XMLSchema">
  <scroll_speed>10</scroll_speed>
  <foreground>true</foreground>

  <layers>

